{
    "git": {
      "productionBranch": "main"
    },
    "builds": [
        {
          "src": "boy-helper-core/package.json",
          "use": "@vercel/node"
        }
      ],
    "routes": [
    { "src": "/(.*)", "dest": "boy-helper-core/$1" }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
  }